{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.5.2",
      "destination": "wwwroot/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.4",
      "destination": "wwwroot/lib/signalr/",
      "files": ["dist/browser/signalr.js"]
    },
    {
      "provider": "unpkg",
      "library": "moment@2.25.3",
      "destination": "wwwroot/lib/moment/",
      "files": [
        "min/moment.min.js",
        "dist/locale/tr.js"
      ]
    }
  ]
}